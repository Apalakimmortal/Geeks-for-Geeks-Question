class Solution{
public:	
	vector<int> getMoreAndLess(int arr[], int n, int x) {
	    int count1 = 0;
	    int count2 = 0;
	    vector<int> ans;
	    
	    for(int i = 0;i<n;i++)
	    {
	        if(arr[i]<=x)
	        {
	            count1++;
	        }
	        if(arr[i]>=x)
	        {
	            count2++;
	        }
	    }
	    ans.push_back(count1);
	    ans.push_back(count2);
	    return ans;
	}
};
